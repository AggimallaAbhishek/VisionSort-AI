{
  "version": 2,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api",
      "destination": "https://visionsort-ai.onrender.com/"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://visionsort-ai.onrender.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
